<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">

    <changeSet author="kleber" id="1">
        <createTable tableName="medicos">
            <column name="id" type="int" autoIncrement="true">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="nome" type="varchar(255)"/>
            <column name="email" type="varchar(255)"/>
            <column name="crm" type="varchar(255)"/>
            <column name="especialidade" type="varchar(255)"/>
            <column name="logradouro" type="varchar(255)"/>
            <column name="bairro" type="varchar(255)"/>
            <column name="cep" type="varchar(255)"/>
            <column name="numero" type="varchar(255)"/>
            <column name="complemento" type="varchar(255)"/>
            <column name="cidade" type="varchar(255)"/>
            <column name="estado" type="varchar(255)"/>
            <column name="uf" type="varchar(255)"/>
        </createTable>
    </changeSet>

</databaseChangeLog>